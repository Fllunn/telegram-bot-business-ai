# Токен Telegram бота
TELEGRAM_TOKEN=your_telegram_bot_token_here

# API ключ OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# ID владельца бота, сюда будут приходить логи
# Можно указать несколько ID через запятую
OWNER_ID=123456789

# Список разрешенных пользователей (через запятую, без пробелов)
# Если оставить пустым, доступ будет у всех пользователей
# Пример: ALLOWED_USERS=123456789,987654321
ALLOWED_USERS=
